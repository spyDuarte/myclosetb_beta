{
  "name": "myclosetb_beta",
  "version": "0.1.0",
  "description": "Sistema de gerenciamento de closet virtual - App iOS/Android",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "build:cli": "tsc",
    "start:cli": "node dist/index.js",
    "dev:cli": "ts-node src/index.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src/**/*.ts mobile/**/*.tsx app/**/*.tsx",
    "format": "prettier --write \"src/**/*.ts\" \"mobile/**/*.tsx\" \"app/**/*.tsx\""
  },
  "keywords": [
    "closet",
    "wardrobe",
    "fashion",
    "organization",
    "react-native",
    "expo",
    "ios",
    "android"
  ],
  "author": "spyDuarte",
  "license": "MIT",
  "dependencies": {
    "expo": "~50.0.0",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "~3.29.0",
    "@react-navigation/native": "^6.1.9",
    "@react-navigation/bottom-tabs": "^6.5.11",
    "@react-navigation/native-stack": "^6.9.17",
    "@expo/vector-icons": "^13.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
    "@react-native-picker/picker": "2.6.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@babel/core": "^7.23.5",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/react": "~18.2.45",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "private": true
}
